extends ../layout

block content
  h2.display-data Promisoffsitestorage Detail
            
  table.table.table-bordered
    tr
      th Property
      th Value
        tr
          td Label
          td= display(Promisoffsitestorage.label)
        tr
          td Description
          td= display(Promisoffsitestorage.description)


  .form-actions
    a.btn.btn-warning(href="/Promisoffsitestorages") Back
    | &nbsp;
    a.btn.btn-primary(href="/Promisoffsitestorages/" + Promisoffsitestorage._id + "/edit") Edit
    | &nbsp;
    a.btn.btn-danger(href="/Promisoffsitestorages/" + Promisoffsitestorage._id + "/delete") Delete