extends ../layout

block content
  body
    form#networkLines-form.form-horizontal.col-md-offset-2.col-md-10(action="/networkLines/" + networkLines._id + "/edit", method="POST")
      fieldset
        legend Edit Networklines

        != form

      .form-group.col-md-offset-2.col-md-10
          a.btn.btn-warning(href="/networkLines") Back
          | &nbsp;
          button.btn.btn-primary(type="submit") Save
