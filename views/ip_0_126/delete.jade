extends ../layout

block content
  h2.display-data Delete Ip_0_126?
            
  table.table.table-bordered
    tr
      th Property
      th Value
        tr
          td Ip
          td= display(ip_0_126.IP)
        tr
          td Iftypeicon
          td= display(ip_0_126.ifTypeIcon)
        tr
          td Ifname
          td= display(ip_0_126.ifName)
        tr
          td Ifdescr
          td= display(ip_0_126.ifDescr)
        tr
          td Ifalias
          td= display(ip_0_126.ifAlias)
        tr
          td Ifadminicon
          td= display(ip_0_126.ifAdminIcon)
        tr
          td Ifopericon
          td= display(ip_0_126.ifOperIcon)
        tr
          td Mac
          td= display(ip_0_126.MAC)
        tr
          td Dns
          td= display(ip_0_126.DNS)
        tr
          td Vlans
          td= display(ip_0_126.VLANs)
        tr
          td Vlanformac
          td= display(ip_0_126.VLANforMAC)
        tr
          td Ifspeed
          td= display(ip_0_126.ifspeed)
        tr
          td Ifpype
          td= display(ip_0_126.ifpype)
        tr
          td Trunkport
          td= display(ip_0_126.TrunkPort)


  form#ip_0_126-form.form-horizontal(action="/ip_0_126s/" + ip_0_126._id + "/delete", method="POST")
    .form-actions
      a.btn.btn-warning(href="/ip_0_126s") Back
      | &nbsp;
      input.btn.btn-danger(type="submit", value="Delete")
